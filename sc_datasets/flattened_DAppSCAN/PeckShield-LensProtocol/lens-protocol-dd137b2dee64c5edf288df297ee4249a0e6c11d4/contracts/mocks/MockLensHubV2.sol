     */
    function initialize(
        uint256 profileId,
        string calldata name,
        string calldata symbol
    ) external;

    /**
     * @notice Mints a follow NFT to the specified address. This can only be called by the hub, and is called
     * upon follow.
     *
     * @param to The address to mint the NFT to.
     */
